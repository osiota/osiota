#!/bin/sh

cd "$(dirname "$0")"

erconfig="config.json"
clog="er-output.log"
if test "x$1" != "x"
then
	erconfig="$1.json"
	name="${1##*/}"
	clog="er-output-${name}.log"
fi

if test -f ./router_main_init.js
then
	./router_main_init.js >${clog} 2>&1
	exit 0
fi


/usr/bin/env nodejs ./main_cli.js --config "${erconfig}" >${clog} 2>&1
if test "x$?" = "x127" || test "x$?" = "x126"
then
	/usr/bin/env node ./main_cli.js --config "${erconfig}" >${clog} 2>&1
fi

